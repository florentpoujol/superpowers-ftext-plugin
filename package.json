{
  "name": "sup-ftext-plugin",
  "version": "0.1.0",
  "description": "Generic text asset for Superpowers, the extensible HTML5 2D+3D game engine.",
  "homepage": "https://github.com/florentpoujol/superpowers-ftext-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/florentpoujol/superpowers-ftext-plugin"
  },
  "author": "Florent Poujol <florent.poujol@gmail.com> (http://florentpoujol.fr)",
  "dependencies": {
    "typescript": "1.5.0-alpha",
    "codemirror": "5.1.x",
    "operational-transform": "^0.2.1",
    "cson-parser": "^1.0.8",
    "domify": "^1.3.1",
    "jade": "^1.9.2",
    "less": "^2.4.0",
    "markdown": "^0.5.0",
    "jsonlint": "^1.6.2",
    "file": "^0.2.2",
    "system": "^0.1.0"
  },
  "keywords": [
    "superpowers",
    "plugin",
    "text",
    "asset",
    "json",
    "cson",
    "html",
    "markdown",
    "jade",
    "css",
    "less",
    "shader"
  ],
  "bugs": {
    "url": "https://github.com/florentpoujol/superpowers-ftext-plugin/issues"
  },
  "scripts": {
    "compilets": "tsc api/fText.ts --out /gitignore/fText.js --target ES6",
    "gendoc": "typedoc --out public/docs/ api/fText.ts --mode file --target ES6 --name \"fText plugin\" --readme tutorial.md --theme minimal",
    "linksrcdev": "mklink /j \"../../app/plugins/florentpoujol/fText\" \"./\"",
    "linksrcrelease": "mklink /j \"../../../release/app/plugins/florentpoujol/fText\" \"./\"",
    "linkprojdev": "mklink /j \"../../data/projects/fText\" \"./project/fText\"",
    "linkprojrelease": "mklink /j \"../../../localprojects/fText\" \"./project/fText\""
  },
  "maintainers": [
    "Florent Poujol <florent.poujol@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "gulp": "^3.9.0"
  }
